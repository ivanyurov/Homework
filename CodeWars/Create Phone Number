#include <string>

std::string createPhoneNumber(const int arr [10]){
  std::string a;
  std::string b="";
  for (int i = 0; i < 10; ++i)
  {
    if (arr[i] == 0) a += "0";
    if (arr[i] == 1) a += "1";
    if (arr[i] == 2) a += "2";
    if (arr[i] == 3) a += "3";
    if (arr[i] == 4) a += "4";
    if (arr[i] == 5) a += "5";
    if (arr[i] == 6) a += "6";
    if (arr[i] == 7) a += "7";
    if (arr[i] == 8) a += "8";
    if (arr[i] == 9) a += "9";    
  }
  b=b+"("+a[0]+a[1]+a[2]+") "+a[3]+a[4]+a[5]+"-"+a[6]+a[7]+a[8]+a[9];
  return b;
}
